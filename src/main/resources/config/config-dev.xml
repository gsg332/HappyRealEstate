<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<!-- mysql -->
	<entry key="classname">com.mysql.jdbc.Driver</entry>
	
	<!-- DB -->
	<entry key="url">jdbc:mysql://localhost:3306/ves?characterEncoding=utf8</entry>
	<entry key="username">ves</entry>
	<entry key="password">inconpass</entry>

	<!-- SMS DB -->
	<entry key="sms.url">jdbc:mysql://localhost:3306/ves</entry>
	<entry key="sms.username">ves</entry>
	<entry key="sms.password">inconpass</entry>
	
	<!-- Video File Directory Location -->
